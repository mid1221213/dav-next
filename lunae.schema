# A schema for the missing "group" objectClass Luna[e]
#   and many others miss
#
# OID number is set as the example numering at the bottom
#   of https://ldap.com/object-identifiers/
#
# OID prefix = 1.3.6.1.4.1 = iso.org.dod.internet.private.enterprise
# 22615 = Luna[e] Project
# 1 = LDAP
# 1 = Luna[e] "app"
# 1 = attributetype, 2 = objectclass
# 1 = incremental

attributetype ( 1.3.6.1.4.1.22615.1.1.1.1 NAME 'gid'
	DESC 'Luna[e] group ID'
	EQUALITY caseIgnoreMatch
	SUBSTR caseIgnoreSubstringsMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.15{256} )

objectclass ( 1.3.6.1.4.1.22615.1.1.2.1 NAME 'lunaeGroup'
	DESC 'Luna[e] group'
	SUP top AUXILIARY
	MUST gid
	MAY ( cn $ mail $ uid ) )

objectclass ( 1.3.6.1.4.1.22615.1.1.2.2 NAME 'lunaeUser'
	DESC 'Luna[e] user'
	SUP top AUXILIARY
	MAY gid )
